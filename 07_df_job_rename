import pandas as pd

# Tworzenie przykładowej ramki danych
df = pd.DataFrame({'AA': [1, 2, 3], 'BB': [4, 5, 6], 'CC': [7, 8, 9]})

# Wyświetlenie ramki danych przed zmianami
print('Przed zmianami:')
print(df)

# Zmiana nazw kolumn 'AA' na 'Pierwsze', 'BB' na 'Drugie', 'CC' na 'Trzecie'
df.rename(columns={'AA': 'Pierwsze', 'BB': 'Drugie', 'CC': 'Trzecie'}, inplace=True)

# Wyświetlenie ramki danych po zmianach
print('Po zmianach:')
print(df)
